{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "img/icon_gray.png",
    "default_popup": "main.html",
    "default_title": "下载管理"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "用于接管浏览器下载功能",
  "icons": {
    "128": "img/icon_gray.png",
    "16": "img/icon_gray.png",
    "48": "img/icon_gray.png"
  },
  "manifest_version": 2,
  "name": "下载管理",
  "optional_permissions": [
    "management"
  ],
  "short_name": "下载管理",
  "permissions": [
    "downloads",
    "downloads.open",
    "downloads.shelf",
    "notifications",
    "background",
    "contextMenus"
  ],
  "version": "1.0.1",
  "homepage_url": "https://github.com/yhl452493373/DownloadManager",
  "author": "yhl452493373",
  "minimum_chrome_version": "54"
}
